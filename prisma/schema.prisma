// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Memecoin {
  id          String   @id @default(cuid())
  name        String   @unique
  symbol      String
  description String?
  logoUrl     String?
  createdAt   DateTime @default(now())
}
